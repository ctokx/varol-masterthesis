    ],
    "permissions": [
        "storage",
        "scripting",
        "system.display",
        "contextMenus",
        "unlimitedStorage"
    ],
    "externally_connectable": {
        "matches": [
            "*://*.aitopia.ai/*",
            "*://*.chatgptextension.ai/*"
        ]
    },
