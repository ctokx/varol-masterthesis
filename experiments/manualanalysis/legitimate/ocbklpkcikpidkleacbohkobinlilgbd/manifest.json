{
"update_url": "https://clients2.google.com/service/update2/crx",

  "name": "__MSG_appName__",
  "description": "__MSG_appDesc__",
  "default_locale": "en",
  "version": "2.1.0",
  "manifest_version": 3,
  "icons": {
    "16": "icons/logo-16.png",
    "32": "icons/logo-32.png",
    "48": "icons/logo-48.png",
    "128": "icons/logo-128.png"
  },
  "action": {},
  "options_page": "options.html",
  "background": {
    "service_worker": "service-worker-loader.js",
    "type": "module"
  },
  "content_scripts": [
    {
      "js": [
        "assets/index.tsx-loader-d66db545.js"
      ],
      "matches": [
        "https://www.youtube.com/*"
      ]
    }
  ],
  "host_permissions": [
    "https://*.chatgpt4youtube.com/",
    "https://*.youtube.com/*"
  ],
  "permissions": [
    "storage",
    "tabs",
    "webRequest"
  ],
  "web_accessible_resources": [
    {
      "matches": [
        "https://*.youtube.com/*"
      ],
      "resources": [
        "scripts/ytInitialPlayerResponse.js",
        "assets/*.js"
      ],
      "use_dynamic_url": false
    },
    {
      "matches": [
        "https://www.youtube.com/*"
      ],
      "resources": [
        "assets/logo-48-10167b30.png",
        "assets/store-71306d8b.js",
        "assets/_commonjsHelpers-725317a4.js",
        "assets/index-86631bbc.js",
        "assets/index.tsx-0dd019d3.js"
      ],
      "use_dynamic_url": true
    }
  ],
  "key": ""
}